{
  "name": "personal-website",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --c production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng build --stats-json",
    "analyze-bundle": "ng build --configuration=analyze-bundle && source-map-explorer dist/**/*.js --no-border-checks",
    "analyze": "webpack-bundle-analyzer dist/PersonalWebsite/stats.json",
    "cypress": "concurrently \"ng serve \" \"cypress open\"",
    "cypress:cli": "concurrently \"ng serve \" \"cypress run\"",
    "integration": "npx playwright test",
    "firebase-release": "firebase deploy --only hosting",
    "test-ui": "npx playwright test --ui",
    "commit": "git cz",
    "deploy": "ng build --c production && npx playwright test && firebase deploy --only hosting",
    "createTheme": "ng generate @angular/material:m3-theme"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^20.3.14",
    "@angular/compiler": "^20.3.14",
    "@angular/core": "^20.3.14",
    "@angular/fire": "^19.2.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^20.3.14",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^20.3.14",
    "@angular/platform-browser-dynamic": "^20.3.14",
    "@angular/router": "^20.3.14",
    "@angular/service-worker": "^20.3.14",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@stripe/stripe-js": "^7.4.0",
    "core-js": "^3.38.1",
    "firebase": "^10.13.0",
    "hammerjs": "^2.0.8",
    "marked": "^15.0.12",
    "mini-css-extract-plugin": "^2.9.1",
    "ngx-countup": "^13.2.0",
    "ngx-markdown": "^19.1.1",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.12",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.14",
    "@eslint/js": "^9.10.0",
    "@playwright/test": "^1.50.1",
    "@types/hammerjs": "^2.0.45",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "git-cz": "^4.9.0",
    "globals": "^15.9.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "playwright": "^1.49.1",
    "playwright-broad-utils": "^1.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.6.0"
  }
}
